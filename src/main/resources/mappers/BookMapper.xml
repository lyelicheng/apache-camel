<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.llye.apache.apachecamel.mapper.BookMapper">
    <insert id="insertBook" parameterType="com.llye.apache.apachecamel.model.Book">
        INSERT INTO book (title, author, genre, published_year)
        VALUES (#{title}, #{author}, #{genre}, #{publishedYear})
    </insert>
</mapper>
